        "default": "12",
        "label": "TRANSMISSION_UMASK"
      },
      {
        "name": "TRANSMISSION_PORT_FORWARDING_ENABLED",
        "default": "true",
        "label": "TRANSMISSION_PORT_FORWARDING_ENABLED"
      },
      {
        "name": "PUID",
        "default": "1000",
        "label": "PUID"
      },
      {
        "name": "PGID",
        "default": "100",
        "label": "PGID"
